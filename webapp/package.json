{
  "name": "webapp",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/node": "^12.0.8",
    "@types/react": "^16.8.20",
    "@types/react-dom": "^16.8.4",
    "@types/react-redux": "^7.1.0",
    "@types/react-router-dom": "^4.3.4",
    "@types/react-router-redux": "^5.0.18",
    "@types/redux-logger": "^3.0.7",
    "autoprefixer": "9.6.0",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "babel-preset-react-app": "^9.0.0",
    "case-sensitive-paths-webpack-plugin": "2.2.0",
    "chalk": "2.4.2",
    "css-loader": "3.0.0",
    "date-fns": "^1.30.1",
    "dcl-tslint-config-standard": "^1.1.0",
    "decentraland-commons": "^5.1.0",
    "decentraland-dapps": "^4.2.0",
    "decentraland-eth": "github:decentraland/decentraland-eth#drophid",
    "decentraland-ui": "^2.3.1",
    "dotenv": "8.0.0",
    "dotenv-expand": "5.1.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "4.0.0",
    "fork-ts-checker-webpack-plugin": "^1.3.7",
    "fs-extra": "8.0.1",
    "html-webpack-plugin": "3.2.0",
    "jest": "24.8.0",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-loader": "3.0.0",
    "prettier": "^1.18.2",
    "promise": "8.0.3",
    "raf": "3.4.1",
    "react": "^16.8.6",
    "react-dev-utils": "^11.0.4",
    "react-dom": "^16.8.6",
    "react-redux": "^7.1.0",
    "react-router-dom": "^5.0.1",
    "react-router-redux": "^5.0.0-alpha.6",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-saga": "^1.0.3",
    "reselect": "^4.0.0",
    "resolve": "1.11.0",
    "source-map-explorer": "^2.0.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "0.23.1",
    "sw-precache-webpack-plugin": "0.11.5",
    "ts-jest": "24.0.2",
    "ts-loader": "^6.0.2",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "tslint": "^5.17.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-language-service": "^0.9.9",
    "tslint-plugin-prettier": "^2.0.1",
    "tslint-react": "^4.0.0",
    "typesafe-actions": "^4.4.1",
    "typescript": "^3.5.2",
    "uglifyjs-webpack-plugin": "^2.1.3",
    "url-loader": "2.0.0",
    "webpack": "4.34.0",
    "webpack-dev-server": "3.7.1",
    "webpack-manifest-plugin": "2.0.4",
    "whatwg-fetch": "3.0.0"
  },
  "scripts": {
    "lint": "tslint -p tsconfig.json",
    "lint:fix": "tslint -p tsconfig.json --fix",
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "build:ci": "npm run lint && node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.(j|t)s?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).(j|t)s?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.tsx?$": "<rootDir>/config/jest/typescriptTransform.js",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs|ts|tsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "web.js",
      "js",
      "web.jsx",
      "jsx",
      "json",
      "node",
      "mjs"
    ],
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.test.json"
      }
    }
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "prettier": {
    "printWidth": 80,
    "singleQuote": true,
    "semi": false
  }
}
